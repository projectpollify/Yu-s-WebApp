{
  "name": "yus-montessori-school-management",
  "version": "1.0.0",
  "description": "A comprehensive school management system for Yus Montessori School",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.4",
    "googleapis": "^126.0.1",
    "stripe": "^13.6.0",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "winston": "^3.10.0",
    "cron": "^2.4.4",
    "axios": "^1.5.0",
    "openai": "^4.8.0",
    "pdf-lib": "^1.17.1",
    "sharp": "^0.32.5",
    "express-validator": "^7.0.1",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "@types/node": "^20.5.9"
  },
  "keywords": [
    "school-management",
    "montessori",
    "education",
    "ai-powered",
    "nodejs",
    "react"
  ],
  "author": "Yus Montessori School",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}